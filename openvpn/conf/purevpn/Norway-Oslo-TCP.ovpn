client
dev tun0
proto tcp
remote vleu-no1-ovpn-tcp.pointtoserver.com 80
nobind
persist-key
persist-tun
cipher AES-256-CBC

key-direction 1

verb 1
comp-lzo
mute 20
float
route-delay 2
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
ifconfig-nowarn
ca /etc/openvpn/conf/purevpn/ca.crt
tls-auth /etc/openvpn/conf/purevpn/Wdc.key 1
