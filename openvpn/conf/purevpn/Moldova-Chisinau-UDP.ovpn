client

dev tun0
remote md1-ovpn-udp.pointtoserver.com 53
proto udp
nobind
persist-key
persist-tun
cipher AES-256-CBC

key-direction 1

verb 1
mute 20
route-method exe
route-delay 2
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
explicit-exit-notify 2
ifconfig-nowarn
ca /etc/openvpn/conf/purevpn/ca.crt
tls-auth /etc/openvpn/conf/purevpn/Wdc.key 1
