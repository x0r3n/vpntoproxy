client
dev tun0
proto tcp
remote usil1-ovpn-tcp.ivacy.net 80
persist-key
persist-tun
ca /etc/openvpn/conf/ivacy/ca.crt
tls-auth /etc/openvpn/conf/ivacy/Wdc.key 1
cipher AES-256-CBC
comp-lzo
verb 1
comp-lzo
comp-lzo
mute 20
route-method exe
route-delay 2
route 0.0.0.0 0.0.0.0
float
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
ifconfig-nowarn
